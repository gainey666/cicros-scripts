  <If Condition="(InbagCount(929) + InbagCount(3383)*2 + InbagCount(858) + InbagCount(2450)) &lt; 100 &amp;&amp; Alchemy.Level &gt;= 60 &amp;&amp; Alchemy.Level &lt; 140 || (InbagCount(3383) + InbagCount(785)) &lt; 20 &amp;&amp; Alchemy.Level &gt;= 60 &amp;&amp; Alchemy.Level &lt; 140" IgnoreCanRun="True">
