    <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\Wetlands_Bruiseweed.xml" />
  </If>
  <If Condition="(InbagCount(3176)*2 + InbagCount(2450)) &lt; 50 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205" IgnoreCanRun="True">
    <CallSubRoutine SubRoutineName="Sell" />
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Farming some more Brairthron.&quot;); " />
    <If Condition="Me.ZoneId != 10" IgnoreCanRun="True">
      <FlyToAction Dismount="True" Location="-10155.53, -40.49891, 33.55697" />
    </If>
    <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\Duskwood_Brairthron.xml" />
  </If>
  <If Condition="(InbagCount(3176)*2 + InbagCount(2450)) &gt;= 50 &amp;&amp; (InbagCount(1622) + InbagCount(3176)*2) &lt; 50 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(3355) + InbagCount(3825) + InbagCount(3825)) &lt; 70 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205" IgnoreCanRun="True">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Farming WildSteel and Bruiseweed.&quot;); " />
    <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\N-STW_WildSteel+Bruiseweed.xml" />
  </If>
  <If Condition="(InbagCount(3176)*2 + InbagCount(2450)) &gt;= 50 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(1622) + InbagCount(3176)*2) &gt;= 50 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(3355) + InbagCount(3825)) &gt;= 70 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(1710) + InbagCount(3356)) &lt; 30 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205" IgnoreCanRun="True">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Farming Kingsblood.&quot;); " />
    <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\N-STW_Kingsblood.xml" />
  </If>
  <If Condition="(InbagCount(3176)*2 + InbagCount(2450)) &gt;= 50 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(1622) + InbagCount(3176)*2) &gt;= 50 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(3355) + InbagCount(3825)) &gt;= 70 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(1710) + InbagCount(3356)) &gt;= 30 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205 || (InbagCount(1710) + InbagCount(3557)) &lt; 20 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205" IgnoreCanRun="True">
