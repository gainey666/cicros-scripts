<?xml version="1.0" encoding="utf-8"?>
<Professionbuddy>
  <!--Intro-->
  <CustomAction Code="Log(System.Drawing.Color.DarkOliveGreen,&quot;Cicro's Herb and Alch 1-525 v0.0&quot;);" />
  <If Condition="TreeRoot.Current.Name != &quot;Gatherbuddy2&quot;" IgnoreCanRun="True">
    <CustomAction Code="SwitchToBot(&quot;Gatherbuddy2&quot;);" />
  </If>
  <If Condition="Herbalism.MaxLevel == 0" IgnoreCanRun="True">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I don't know Herbalism. Going to learn it&quot;); " />
    <CallSubRoutine SubRoutineName="TrainHerb" />
  </If>
  <If Condition="Alchemy.MaxLevel == 0" IgnoreCanRun="True">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I don't know Alchemy. Going to learn it&quot;); " />
    <CallSubRoutine SubRoutineName="TrainAlch" />
    <WaitAction Condition="false" Timeout="3000" />
  </If>
  <CallSubRoutine SubRoutineName="CheckSkill" />
  <!--Subs-->
  <SubRoutine SubRoutineName="TrainAlchRecipe">
    <If Condition="Me.MapId == 0" IgnoreCanRun="True">
      <If Condition="Me.IsAlliance" IgnoreCanRun="True">
        <If Condition="DistanceTo (-9073.915, 152.9712, 112.3703) &lt; DistanceTo (-5026.759, -828.7828, 495.3112)" IgnoreCanRun="False">
          <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need to train Alchemy. Going to train in Stormwind.&quot;); " />
          <FlyToAction Dismount="True" Location="-9073.915, 152.9712, 112.3703" />
          <TrainSkillAction NpcEntry="1215" Location="-9056.231, 152.6291, 115.0333" />
        </If>
        <If Condition="DistanceTo (-5026.759, -828.7828, 495.3112) &lt; DistanceTo (-9073.915, 152.9712, 112.3703)" IgnoreCanRun="False">
          <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need to train Alchemy. Going to train in Ironforge.&quot;); " />
          <If Condition="Me.ZoneId != 1537" IgnoreCanRun="True">
            <FlyToAction Dismount="False" Location="-5026.759, -828.7828, 495.3112" />
            <TrainSkillAction NpcEntry="5177" Location="-4863.042, -1240.297, 501.7242" />
            <WaitAction Condition="false" Timeout="2000" />
          </If>
          <If Condition="Me.ZoneId == 1537" IgnoreCanRun="True">
            <TrainSkillAction NpcEntry="5177" Location="-4863.042, -1240.297, 501.7242" />
            <WaitAction Condition="false" Timeout="2000" />
          </If>
        </If>
      </If>
    </If>
  </SubRoutine>
  <SubRoutine SubRoutineName="TrainHerb">
    <If Condition="Me.MapId == 0" IgnoreCanRun="True">
      <If Condition="Me.IsAlliance" IgnoreCanRun="True">
        <If Condition="DistanceTo (-3587.237, -858.5277, 4.071148) &lt; DistanceTo (-9059.666, 148.5659, 115.0341)" IgnoreCanRun="False">
          <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need to train Herbalism. Going to train in Wetland.&quot;); " />
          <FlyToAction Dismount="True" Location="-3587.237, -858.5277, 4.071148" />
          <TrainSkillAction NpcEntry="1458" Location="-3584.83, -854.755, 3.760071" />
        </If>
        <If Condition="DistanceTo (-9059.666, 148.5659, 115.0341) &lt; DistanceTo (-3587.237, -858.5277, 4.071148)" IgnoreCanRun="False">
          <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need to train Herbalism. Going to train outside SW.&quot;); " />
          <FlyToAction Dismount="True" Location="-9073.915, 152.9712, 112.3703" />
          <TrainSkillAction NpcEntry="1218" Location="-9059.666, 148.5659, 115.0341" />
        </If>
      </If>
    </If>
  </SubRoutine>
  <SubRoutine SubRoutineName="TrainAlch">
    <If Condition="Me.MapId == 0" IgnoreCanRun="True">
      <If Condition="Me.IsAlliance" IgnoreCanRun="True">
        <If Condition="DistanceTo (-9073.915, 152.9712, 112.3703) &lt; DistanceTo (-5026.759, -828.7828, 495.3112)" IgnoreCanRun="False">
          <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need to train Alchemy. Going to train in Stormwind.&quot;); " />
          <FlyToAction Dismount="True" Location="-9073.915, 152.9712, 112.3703" />
          <TrainSkillAction NpcEntry="1215" Location="-9056.231, 152.6291, 115.0333" />
        </If>
        <If Condition="DistanceTo (-5026.759, -828.7828, 495.3112) &lt; DistanceTo (-9073.915, 152.9712, 112.3703)" IgnoreCanRun="False">
          <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need to train Alchemy. Going to train in Ironforge.&quot;); " />
          <If Condition="Me.ZoneId != 1537" IgnoreCanRun="True">
            <FlyToAction Dismount="False" Location="-5026.759, -828.7828, 495.3112" />
            <TrainSkillAction NpcEntry="5177" Location="-4863.042, -1240.297, 501.7242" />
            <WaitAction Condition="false" Timeout="2000" />
            <MoveToAction Location="-5026.759, -828.7828, 495.3112" MoveType="Location" Pathing="Navigator" Entry="0" />
          </If>
          <If Condition="Me.ZoneId == 1537" IgnoreCanRun="True">
            <TrainSkillAction NpcEntry="5177" Location="-4863.042, -1240.297, 501.7242" />
            <WaitAction Condition="false" Timeout="2000" />
            <MoveToAction Location="-5026.759, -828.7828, 495.3112" MoveType="Location" Pathing="Navigator" Entry="0" />
          </If>
        </If>
      </If>
    </If>
  </SubRoutine>
  <SubRoutine SubRoutineName="Sell">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Time to clean the bags!&quot;); " />
    <If Condition="Me.IsAlliance" IgnoreCanRun="True">
      <If Condition="Alchemy.Level &gt;= 60 &amp;&amp; Alchemy.Level &lt; 61" IgnoreCanRun="True">
        <SellItemAction Sell="All" NpcEntry="0" Location="0, 0, 0" SellItemType="Greys" ItemID="" Count="0" />
        <SellItemAction Sell="All" NpcEntry="5178" Location="-4856.877, -1237.258, 501.7241" SellItemType="Specific" ItemID="2447,765,2449" Count="0" />
        <WaitAction Condition="false" Timeout="3000" />
      </If>
      <If Condition="Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 145 &amp;&amp; InbagCount(929) &gt;= 1" IgnoreCanRun="True">
        <SellItemAction Sell="All" NpcEntry="0" Location="0, 0, 0" SellItemType="Greys" ItemID="" Count="0" />
        <SellItemAction Sell="All" NpcEntry="66" Location="-9492.55, 81.4931, 56.22693" SellItemType="Specific" ItemID="2447,765,2449,929,118,3383,858,3383,785,2453,2452" Count="0" />
        <WaitAction Condition="false" Timeout="3000" />
      </If>
    </If>
  </SubRoutine>
  <SubRoutine SubRoutineName="CheckSkill">
    <If Condition="Herbalism.Level &gt;= Herbalism.MaxLevel - 25 &amp;&amp; Herbalism.Level &lt;= Herbalism.MaxLevel" IgnoreCanRun="True">
      <CallSubRoutine SubRoutineName="TrainHerb" />
    </If>
    <If Condition="Alchemy.Level &gt;= Alchemy.MaxLevel - 25 &amp;&amp; Alchemy.Level &lt;= Alchemy.MaxLevel" IgnoreCanRun="True">
      <CallSubRoutine SubRoutineName="TrainAlch" />
    </If>
  </SubRoutine>
  <SubRoutine SubRoutineName="BuyVial">
    <If Condition="InbagCount(3371) &lt; 90" IgnoreCanRun="True">
      <If Condition="Me.IsAlliance" IgnoreCanRun="True">
        <If Condition="Me.MapId == 0" IgnoreCanRun="True">
          <If Condition="DistanceTo (-5026.759, -828.7828, 495.3112) &lt; DistanceTo (-9073.915, 152.9712, 112.3703)" IgnoreCanRun="False">
            <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;I need Vials!. Going to Ironforge.&quot;); " />
            <If Condition="Me.ZoneId != 1537" IgnoreCanRun="True">
              <FlyToAction Dismount="True" Location="-5026.759, -828.7828, 495.3112" />
              <BuyItemAction NpcEntry="5178" Location="-4856.877, -1237.258, 501.724" ItemID="3371" BuyItemType="SpecificItem" Count="20" BuyAdditively="False" />
              <WaitAction Condition="false" Timeout="2000" />
            </If>
            <If Condition="Me.ZoneId == 1537" IgnoreCanRun="True">
              <BuyItemAction NpcEntry="5178" Location="-4856.877, -1237.258, 501.724" ItemID="3371" BuyItemType="SpecificItem" Count="20" BuyAdditively="False" />
              <WaitAction Condition="false" Timeout="2000" />
            </If>
          </If>
          <If Condition="DistanceTo (-9073.915, 152.9712, 112.3703) &lt; DistanceTo (-5026.759, -828.7828, 495.3112)" IgnoreCanRun="False">
            <FlyToAction Dismount="True" Location="-9492.55, 81.4931, 56.22646" />
            <BuyItemAction NpcEntry="66" Location="-9492.55, 81.4931, 56.22646" ItemID="3371" BuyItemType="SpecificItem" Count="20" BuyAdditively="False" />
          </If>
        </If>
      </If>
    </If>
  </SubRoutine>
  <SubRoutine SubRoutineName="CheckHerb">
    <If Condition="Me.IsAlliance" IgnoreCanRun="True">
      <If Condition="Herbalism.Level &lt; 71 &amp;&amp; Alchemy.Level &gt; 60" IgnoreCanRun="True">
        <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;We are not lvl 70 in Herb yet, farming abit&quot;); " />
        <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\Dun-Morogh_Peacebloom+Silverleaf.xml" />
      </If>
      <If Condition="Herbalism.Level &gt;= 71 &amp;&amp; Herbalism.Level &lt; 86 &amp;&amp; Alchemy.Level &gt; 140" IgnoreCanRun="True">
        <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;We are not lvl 85 in Herb yet, farming abit&quot;); " />
        <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\Hillsbrad_Mageroyal+Brairthorn.xml" />
      </If>
    </If>
  </SubRoutine>
  <!--Training on the go-->
  <While Condition="Herbalism.Level &gt;= Herbalism.MaxLevel - 25 &amp;&amp; Herbalism.Level &lt;= Herbalism.MaxLevel" IgnoreCanRun="True">
    <CallSubRoutine SubRoutineName="TrainHerb" />
  </While>
  <!--Whiles for Alchemy-->
  <While Condition="(InbagCount(118) + InbagCount(2447)) &gt;= 59 &amp;&amp; (InbagCount(118) + InbagCount(765)) &gt;= 59 &amp;&amp; Alchemy.Level &gt;= 1 &amp;&amp; Alchemy.Level &lt; 60" IgnoreCanRun="True">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Minor Healing Potions&quot;); " />
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 60" IgnoreCanRun="False">
      <CastSpellAction RepeatType="Craftable" Repeat="59" Entry="2330" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="Sell" />
    <CallSubRoutine SubRoutineName="TrainAlch" />
    <If Condition="Me.ZoneId == 1537" IgnoreCanRun="True">
      <MoveToAction Location="-5026.759, -828.7828, 495.3112" MoveType="Location" Pathing="Navigator" Entry="0" />
    </If>
  </While>
  <While Condition="(InbagCount(929) + InbagCount(3383)*2 + InbagCount(858) + InbagCount(2450)) &gt;= 100 &amp;&amp; (InbagCount(3383) + InbagCount(785)) &gt;= 20 &amp;&amp; (InbagCount(2453) + InbagCount(929)) &gt;= 34 &amp;&amp; Alchemy.Level &gt;= 60 &amp;&amp; Alchemy.Level &lt; 140" IgnoreCanRun="False">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Lets get Alchemy to lvl 140&quot;); " />
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 105" IgnoreCanRun="False">
      <CastSpellAction RepeatType="Craftable" Repeat="50" Entry="2337" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 110" IgnoreCanRun="False">
      <If Condition="!HasRecipe(3171)" IgnoreCanRun="True">
        <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
        <WaitAction Condition="false" Timeout="2000" />
        <If Condition="!HasRecipe(3171)" IgnoreCanRun="True">
          <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
          <WaitAction Condition="false" Timeout="2000" />
        </If>
      </If>
      <CastSpellAction RepeatType="Craftable" Repeat="5" Entry="3171" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 140" IgnoreCanRun="False">
      <If Condition="!HasRecipe(3447)" IgnoreCanRun="True">
        <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
        <WaitAction Condition="false" Timeout="2000" />
        <If Condition="!HasRecipe(3447)" IgnoreCanRun="True">
          <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
          <WaitAction Condition="false" Timeout="2000" />
        </If>
      </If>
      <CastSpellAction RepeatType="Craftable" Repeat="33" Entry="3447" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="TrainAlch" />
    <If Condition="Me.ZoneId == 1537" IgnoreCanRun="True">
      <MoveToAction Location="-5026.759, -828.7828, 495.3112" MoveType="Location" Pathing="Navigator" Entry="0" />
    </If>
  </While>
  <While Condition="(InbagCount(3176)*2 + InbagCount(2450)) &gt;= 50 &amp;&amp; (InbagCount(1622) + InbagCount(3176)*2) &gt;= 50 &amp;&amp; (InbagCount(3355) + InbagCount(3825)) &gt;= 70 &amp;&amp; (InbagCount(1710) + InbagCount(3356)) &gt;= 30 &amp;&amp; (InbagCount(1710) + InbagCount(3557)) &gt;= 20 &amp;&amp; (InbagCount(2046) + InbagCount(3825)) &gt;= 40 &amp;&amp; Alchemy.Level &gt;= 140 &amp;&amp; Alchemy.Level &lt; 205" IgnoreCanRun="False">
    <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Lets get Alchemy to lvl 205&quot;); " />
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 155" IgnoreCanRun="False">
      <If Condition="!HasRecipe(3176)" IgnoreCanRun="True">
        <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
        <WaitAction Condition="false" Timeout="2000" />
      </If>
      <CastSpellAction RepeatType="Craftable" Repeat="20" Entry="3176" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 175" IgnoreCanRun="False">
      <If Condition="!HasRecipe(7181)" IgnoreCanRun="True">
        <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
