      </If>
      <CastSpellAction RepeatType="Craftable" Repeat="20" Entry="7181" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="BuyVial" />
    <While Condition="Alchemy.Level &lt; 205" IgnoreCanRun="False">
      <If Condition="!HasRecipe(3825)" IgnoreCanRun="True">
        <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
        <WaitAction Condition="false" Timeout="2000" />
        <If Condition="!HasRecipe(3447)" IgnoreCanRun="True">
          <CallSubRoutine SubRoutineName="TrainAlchRecipe" />
          <WaitAction Condition="false" Timeout="2000" />
        </If>
      </If>
      <CastSpellAction RepeatType="Craftable" Repeat="60" Entry="3825" CastOnItem="False" ItemType="Chest" ItemId="0" />
    </While>
    <CallSubRoutine SubRoutineName="TrainAlch" />
    <If Condition="Me.ZoneId == 1537 &amp;&amp; Me.IsAlliance" IgnoreCanRun="True">
      <MoveToAction Location="-5026.759, -828.7828, 495.3112" MoveType="Location" Pathing="Navigator" Entry="0" />
