  <!--If's for Herbalism-->
  <If Condition="(InbagCount(118) + InbagCount(2447)) &lt; 59 &amp;&amp; Alchemy.Level &gt;= 1 &amp;&amp; Alchemy.Level &lt; 60 || (InbagCount(118) + InbagCount(765)) &lt; 59 &amp;&amp; Alchemy.Level &gt;= 1 &amp;&amp; Alchemy.Level &lt; 60" IgnoreCanRun="True">
    <If Condition="Me.IsAlliance" IgnoreCanRun="True">
      <CustomAction Code="Log(System.Drawing.Color.YellowGreen,&quot;Farming Peacebloom and Silverleaf.&quot;); " />
      <LoadProfileAction ProfileType="Honorbuddy" Path="Cicro\Dun-Morogh_Peacebloom+Silverleaf.xml" />
